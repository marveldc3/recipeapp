{
  "version": 2,
  "builds": [
    {
      "src": "./src",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "./public/index.html",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}