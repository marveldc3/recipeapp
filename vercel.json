{
  "version": 2,
  "builds": [
    {
      "src": "./src",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "./public/index.html",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      },
      "redirects": {
        "source": "/",
        "destination": "/index.html",
        "permanent": true
      }
    }
  ]
}